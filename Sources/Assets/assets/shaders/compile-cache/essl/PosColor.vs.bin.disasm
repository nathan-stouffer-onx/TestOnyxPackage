precision highp int;
precision highp float;
in highp vec4 a_color0;
in highp vec3 a_position;
out highp vec4 v_color0;
uniform highp mat4 u_view;
uniform highp mat4 u_proj;
void main ()
{
  highp vec4 tmpvar_1;
  tmpvar_1.w = 1.0;
  tmpvar_1.xyz = a_position;
  gl_Position = (u_proj * (u_view * tmpvar_1));
  v_color0 = a_color0;
}

