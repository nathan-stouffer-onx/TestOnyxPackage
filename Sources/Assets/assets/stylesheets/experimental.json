{
    "name": "backcountry-satellite-summer",
    "version": 8,
    "sprite": "https://assets.production.onxmaps.com/v1/carto/onxspritesheet/backcountry/1674677385/sprites",
    "glyphs": "https://assets.production.onxmaps.com/v1/carto/onxglyphset/backcountry/{fontstack}/{range}.pbf",
    "sources": {
        "elevation": {
            "minzoom": 0,
            "maxzoom": 15,
            "tiles": [
                "https://tileserver.production.onxmaps.com/v1/elevation-v2/latest/{z}/{x}/{y}.png"
            ],
            "type": "raster-dem",
            "encoding": "terrarium",
            "tileSize": 256
        },
        "aerial": {
            "minzoom": 0,
            "maxzoom": 19,
            "tiles": [
                "https://tileserver.production.onxmaps.com/v1/aerial/latest/{z}/{x}/{y}.jpg"
            ],
            "type": "raster",
            "tileSize": 256
        },
        "base": {
            "type": "vector",
            "tiles": [
                "https://tileserver.production.onxmaps.com/v1/base/v0/{z}/{x}/{y}.pbf"
            ],
            "minzoom": 2,
            "maxzoom": 13
        }
    },
    "expressions": {
        "exaggeration-expr": {
            "number": [
                "interpolate",
                [
                    "linear"
                ],
                [
                    "pitch"
                ],
                0,
                0,
                45,
                2
            ]
        }
    },
    "layouts": {
        "sym-layout": {
            "symbol-layout": {
                "text-field": [
                    "get",
                    "label"
                ],
                "text-size": 17.5,
                "symbol-placement": "line",
                "text-font": [
                    "Open Sans SemiBold"
                ],
                "visibility": "none"
            }
        }
    },
    "paints": {
        "sym-paint": {
            "symbol-paint": {
                "text-color": "hsl(291, 17%, 60%)",
                "text-halo-color": "#FFFFFFFF",
                "text-shadow-color": "#FFFFFF7F"
            }
        }
    },
    "terrain": {
        "source": "elevation",
        "exaggeration": [
            "context",
            "exaggeration-expr"
        ]
    },
    "layers": [
        {
            "id": "background",
            "type": "background",
            "layout": {
                "visibility": "none"
            },
            "paint": {
                "background-color": "hsl(0, 0%, 97%)"
            }
        },
        {
            "id": "tileserver_aerial",
            "source": "aerial",
            "type": "raster",
            "minzoom": 0
        },
        {
            "id": "admin_bounds$country",
            "source": "base",
            "source-layer": "admin_boundaries",
            "type": "line",
            "minzoom": 2,
            "filter": [
                "match",
                [
                    "get",
                    "data:type"
                ],
                [
                    "country"
                ],
                true,
                false
            ],
            "layout": {
                "line-cap": "round",
                "line-join": "round"
            },
            "paint": {
                "line-color": "hsl(0, 0%, 60%)",
                "line-width": 2.5
            }
        },
        {
            "id": "admin_bounds$state",
            "source": "base",
            "source-layer": "admin_boundaries",
            "type": "line",
            "minzoom": 4,
            "filter": [
                "match",
                [
                    "get",
                    "data:type"
                ],
                [
                    "state"
                ],
                true,
                false
            ],
            "layout": {
                "line-cap": "round",
                "line-join": "round"
            },
            "paint": {
                "line-color": [
                    "interpolate",
                    [
                        "linear"
                    ],
                    [
                        "zoom"
                    ],
                    4,
                    "hsl(0, 0%, 70%)",
                    13,
                    "hsl(0, 0%, 60%)"
                ],
                "line-width": 2.5,
                "line-dasharray": [
                    2,
                    4
                ]
            }
        },
        {
            "id": "water$area_fill",
            "source": "base",
            "source-layer": "water_areas",
            "type": "fill",
            "minzoom": 2,
            "paint": {
                "fill-color": "#89BDF9",
                "fill-opacity": 0.25
            }
        },
        {
            "id": "admin_bounds$county_border_label",
            "source": "base",
            "source-layer": "admin_boundaries",
            "type": "symbol",
            "minzoom": 12,
            "maxzoom": 22,
            "filter": [
                "==",
                [
                    "get",
                    "data:type"
                ],
                "county"
            ],
            "layout": {
                "base": "sym-layout"
            },
            "paint": {
                "base": "sym-paint"
            }
        },
        {
            "id": "contour$labels$major",
            "source": "elevation",
            "type": "contour-label",
            "minzoom": 12,
            "maxzoom": 23,
            "layout": {
                "text-size": 14,
                "symbol-placement": "point",
                "text-font": [
                    "Open Sans SemiBold"
                ],
                "contour-label-period": 1000,
                "contour-label-units": "feet",
                "contour-label-suffix": "ft"
            },
            "paint": {
                "text-color": "hsl(291, 100%, 70%)",
                "text-shadow-color": "hsl(291, 20%, 10%)"
            }
        },
        {
            "id": "contour$labels$minor",
            "source": "elevation",
            "type": "contour-label",
            "minzoom": 15,
            "maxzoom": 23,
            "layout": {
                "text-size": 12,
                "symbol-placement": "point",
                "text-font": [
                    "Open Sans SemiBold"
                ],
                "contour-label-period": 250,
                "contour-label-skip-period": 1000,
                "contour-label-units": "feet",
                "contour-label-suffix": "ft"
            },
            "paint": {
                "text-color": "hsl(191, 100%, 70%)",
                "text-shadow-color": "hsl(191, 20%, 10%)"
            }
        }
    ]
}